{
  "name": "seed-it",
  "version": "0.0.30",
  "description": "Capture database traffic from integration tests and generate migrations and seeders",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "seed-it": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "prepare": "npm run build",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:e2e": "test/scripts/test-db.sh start && DEBUG=true jest test/e2e && test/scripts/test-db.sh stop",
    "test:e2e:watch": "DEBUG=true jest test/e2e --watch",
    "example": "node examples/basic-usage.js"
  },
  "keywords": [
    "database",
    "seeder",
    "migration",
    "testing",
    "postgresql",
    "pg",
    "fixtures"
  ],
  "author": "Danny Sandi",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2",
    "commander": "^11.0.0",
    "pg": "^8.11.0",
    "pgsql-ast-parser": "^12.0.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "@types/pg": "^8.11.10",
    "@types/winston": "^2.4.4",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
